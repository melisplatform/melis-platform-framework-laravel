{
  "name": "melisplatform/melis-platform-framework-laravel",
  "description": "Melis Platform Frameworks Laravel",
  "type": "melisplatform-module",
  "license": "OSL-3.0",
  "homepage": "https://github.com/melisplatform/melis-platform-framework-laravel",
  "support": {
    "source": "https://github.com/melisplatform/melis-platform-framework-laravel",
    "issues": "https://github.com/melisplatform/melis-platform-framework-laravel/issues"
  },
  "keywords": [
    "cms",
    "melis",
    "zf2",
    "module"
  ],
  "require": {
    "melisplatform/melis-core": "^3.0",
    "laravel/framework": "^5.8",
    "fideloper/proxy": "^4.0",
    "laravel/tinker": "^1.0"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "../../../thirdparty/laravel/app",
      "MelisPlatformFrameworkLaravel\\": "src/"
    },
    "classmap": [
      "../../../thirdparty/laravel/database/seeds",
      "../../../thirdparty/laravel/database/factories"
    ]
  }
}
