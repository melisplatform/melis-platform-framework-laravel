{
	"name": "melisplatform/melis-platform-framework-laravel",
	"description": "Melis Platform Frameworks Laravel",
	"type": "melisplatform-module",
	"license": "OSL-3.0",
	"homepage": "https://github.com/melisplatform/melis-platform-framework-laravel",
	"support": {
		"source": "https://github.com/melisplatform/melis-platform-framework-laravel",
		"issues": "https://github.com/melisplatform/melis-platform-framework-laravel/issues"
	},
	"keywords": [
		"cms",
		"melis",
		"zf2",
		"module",
		"laravel"
	],
	"extra": {
		"module-name": "MelisPlatformFrameworkLaravel",
		"melis-module": false
	},
	"require": {
		"php": "^8.1|^8.3",
		"melisplatform/melis-platform-frameworks": "*",
		"fideloper/proxy": "^4.4",
		"laravel/framework": "^9.52",
		"laravel/tinker": "^2.9",
		"laravelcollective/html":"^6.4",
		"nwidart/laravel-modules": "^9.0",
		"fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/sanctum": "^3.3",
		"nunomaduro/collision": "^7.10",
		"spatie/laravel-ignition": "^1.0",
		"laravel/sail": "^1.27"
	},
	"require-dev": {
		"filp/whoops": "^2.15",
		"fakerphp/faker": "^1.23",
		"mockery/mockery": "^1.6",
		"phpunit/phpunit": "^10.5"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": [
				"../../../thirdparty/Laravel/app"
			],
			"Modules\\": [
				"../../../thirdparty/Laravel/Modules"
			],
			"MelisPlatformFrameworkLaravel\\": "/"
		}
	}
}
