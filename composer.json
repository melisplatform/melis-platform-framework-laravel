{
	"name": "melisplatform/melis-platform-framework-laravel",
	"description": "Melis Platform Frameworks Laravel",
	"type": "melisplatform-module",
	"license": "OSL-3.0",
	"homepage": "https://github.com/melisplatform/melis-platform-framework-laravel",
	"support": {
		"source": "https://github.com/melisplatform/melis-platform-framework-laravel",
		"issues": "https://github.com/melisplatform/melis-platform-framework-laravel/issues"
	},
	"keywords": [
		"cms",
		"melis",
		"zf2",
		"module",
		"laravel"
	],
	"extra": {
		"module-name": "MelisPlatformFrameworkLaravel",
		"melis-module": false
	},
	"require": {
		"php": "^7.3|^8.0",
		"melisplatform/melis-platform-frameworks": "^5.0",
		"fideloper/proxy": "^4.0",
		"laravel/framework": "^8.83",
		"laravel/tinker": "^2.7",
		"laravelcollective/html": "^6.3",
		"nwidart/laravel-modules": "^8.0",
		"fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/sanctum": "^2.11"
	},
	"require-dev": {
		"filp/whoops": "^2.0",
		"fakerphp/faker": "^1.9.1",
		"laravel/sail": "^1.0.1",
		"mockery/mockery": "^1.4.4",
		"nunomaduro/collision": "^5.10",
		"phpunit/phpunit": "^9.5.10"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": [
				"../../../thirdparty/Laravel/app"
			],
			"Modules\\": [
				"../../../thirdparty/Laravel/Modules"
			],
			"MelisPlatformFrameworkLaravel\\": "/"
		}
	}
}
